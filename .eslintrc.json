{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"plugin:react/recommended",
		"standard"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"rules": {
		"semi": "off",
		"space-before-function-paren": 0,
		"react/prop-types": 0,
		"object-curly-spacing": 0,
		"no-useless-constructor": 0,
		"no-useless-return": 0,
		"indent": ["error", 2]
	}
}